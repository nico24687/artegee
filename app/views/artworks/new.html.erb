<%= form_for [@user, @artwork], html: { multipart: true } do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image %>
  <%= f.file_field  :image %>

  <%= f.label :width %>
  <%= f.number_field :width %>

  <%= f.label :height %>
  <%= f.number_field :height %>

  <%= f.submit "Submit" %>
<% end %>

