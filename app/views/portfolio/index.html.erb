<div class="profile-header text-center" style="background-image: url(/assets/iceland.jpg);">
  <div class="container">
    <div class="container-inner">
       <%= image_tag @user.avatar.url(:thumb), class: "panel-profile-img"%>
      <h3 class="profile-header-user"><%= @user.first_name%> <%= @user.last_name%></h3>
      <p class="profile-header-bio">
        <%= @user.bio %>
      </p>
    </div>
  </div>

  <nav class="profile-header-nav">
    <ul class="nav nav-tabs">
      <li class="active">
        <a href="#">Full Collection</a>
      </li>
    </ul>
  </nav>
</div>




