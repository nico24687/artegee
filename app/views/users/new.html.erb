<%= form_for @user do |f| %>

  <%= f.label :firstname %>
  <%= f.text_field :first_name %>

  <%= f.label :lastname %>
  <%= f.text_field :last_name %>

  <%= f.label :user_name %>
  <%= f.text_field :user_name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :bio %>
  <%= f.text_area :bio %>

  <%= f.submit "Sign Up" %>
<% end %>

